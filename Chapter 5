usernames = ['Jayden', 'Omar', 'luke', 'preston', 'nate']

for username in usernames:
    if username == 'Jayden':
        print(f"Hello Jayden, would you like to see a status report?")
    else:
        print(f"Hello {username}, thank you for logging in again.")
